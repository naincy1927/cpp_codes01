#include <iostream>
using namespace std ;
int main() {
int n,i,count;
  count=0;
  cout<<"enter any number of your choice";
  cin>>n;
  for(i=1;i<=n;i++){
    if(n%i==0){
      count = count+1;
    }else{
      count=count;
    }
  }if(count==2){
    cout<<"number is a prime number ";
  }else{
    cout<<"not";
  }
}
